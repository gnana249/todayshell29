#! /bin/bash
#
#

echo "Enter any  character in ato z : "
read char

case $char in
	'a' ) echo "it is a vowel" ;;
	'e' ) echo "if is a vowel" ;;
	'i' ) echo "it is a vowel" ;;
	'o' ) echo "it is a vowel" ;;
	'u' ) echo "it is a vowel" ;;
	'*' ) echo "change character and use lower case character" ;;
esac

	

	  


	 
